{
  "name": "ahhachul",
  "packageManager": "yarn@3.5.1",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "lint:fix": "eslint --fix",
    "g:lint": "yarn workspaces foreach -pv run lint",
    "g:lint:fix": "yarn workspaces foreach -pv run lint --fix",
    "g:typecheck": "yarn workspaces foreach -pv run typecheck",
    "g:test": "yarn workspaces foreach -pv run test",
    "storybook": "yarn workspace @ahhachul/ui storybook"
  },
  "devDependencies": {
    "@emotion/eslint-plugin": "^11.11.0",
    "@types/jest": "^29.5.1",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  }
}
